<?xml version="1.0"?>
<TMConfig>
  <TMSetup>
    <UseAppDataFolder>false</UseAppDataFolder>
    <TMLibraryDataVirtualPath>..\..</TMLibraryDataVirtualPath>
    <XmlLibrariesPath>TM_Libraries</XmlLibrariesPath>
    <UserDataPath>User_Data</UserDataPath>
    <LibrariesUploadedFiles>LibrariesUploadedFiles</LibrariesUploadedFiles>
    <EnableGZipForWebServices>true</EnableGZipForWebServices>
    <Enable304Redirects>true</Enable304Redirects>
  </TMSetup>
  <TMSecurity>
    <Show_ContentToAnonymousUsers>false</Show_ContentToAnonymousUsers>
    <SSL_RedirectHttpToHttps>true</SSL_RedirectHttpToHttps>
    <EvalAccounts_Enabled>true</EvalAccounts_Enabled>
    <EvalAccounts_Days>15</EvalAccounts_Days>
    <REST_AllowCrossDomainAccess>false</REST_AllowCrossDomainAccess>
    <SingleSignOn_Enabled>false</SingleSignOn_Enabled>
    <Sanitize_HtmlContent>false</Sanitize_HtmlContent>
    <Default_AdminUserName>admin</Default_AdminUserName>
    <Default_AdminPassword>!!tmadmin</Default_AdminPassword>
    <Default_AdminEmail>TM_alerts@securityinnovation.com</Default_AdminEmail>
  </TMSecurity>
  <WindowsAuthentication>
    <Enabled>false</Enabled>
    <ReaderGroup>TM_Reader</ReaderGroup>
    <EditorGroup>TM_Editor</EditorGroup>
    <AdminGroup>TM_Admin</AdminGroup>
  </WindowsAuthentication>
  <Git>
    <AutoCommit_UserData>false</AutoCommit_UserData>
    <AutoCommit_LibraryData>false</AutoCommit_LibraryData>
  </Git>
  <OnInstallation>
    <ForceAdminPasswordReset>false</ForceAdminPasswordReset>
    <DefaultLibraryToInstall_Name />
    <DefaultLibraryToInstall_Location />
  </OnInstallation>
  <VirtualArticles>
    <AutoRedirectIfGuidNotFound>false</AutoRedirectIfGuidNotFound>
    <AutoRedirectTarget>https://teammentor.net/article/</AutoRedirectTarget>
  </VirtualArticles>
</TMConfig>